#include<stdio.h>
int main()
{
    int n,a,i,big;
    printf("Enter the number of elements:\n ");
    scanf("%d", &n);
    printf("if n value=%d then %d numbers are \n",n,n);
    for(i=1;i<=n;i++)
    {
    scanf("%d",&a);
    if(a>big||i==1)
    {
    big=a;
    }
    }
    printf("%d ",big);
return 0;
}
